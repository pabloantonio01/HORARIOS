{
    "version": 2,
    "builds": [
      { "src": "backend/server.js", "use": "@vercel/node" },
      { "src": "frontend/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "js/server.js" },
      { "src": "/(.*)", "dest": "html/pagina_registro.html" }
    ]
  }
  